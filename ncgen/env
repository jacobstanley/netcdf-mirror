K="-k1"
F="c"
#B="-B12"
#NCGEN="./.libs/ncgen"
NCGEN="./ncgen"
alias q0="$NCGEN -d $K -lb $B $F.cdl"
alias qc="$NCGEN -d $K -lc $B $F.cdl"
alias qq="gdb --args $NCGEN -d $K -lb $B $F.cdl"
alias qqc="gdb --args $NCGEN -d $K -lc $B $F.cdl"
alias qv="valgrind --leak-check=full $NCGEN -d $K -lb $B $F.cdl"



